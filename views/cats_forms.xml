<?xml version="1.0" encoding="UTF-8"?>
<odoo>

  <record model="ir.ui.view" id="cats_cat_form">
    <field name="name">cats.cat.form</field>
    <field name="model">cats.cat</field>
    <field name="priority" eval="1"/>
    <field name="arch" type="xml">
      <form>
        <sheet>
          <group>
            <field name="name"/>
            <field name="color"/>
            <field name="type"/>
          </group>
        </sheet>
      </form>
    </field>
  </record>

  <record model="ir.ui.view" id="cats_cat_type_form">
    <field name="name">cats.cat.type.form</field>
    <field name="model">cats.cat.type</field>
    <field name="priority" eval="1"/>
    <field name="arch" type="xml">
      <form>
        <sheet>
          <group>
            <field name="name"/>
          </group>
        </sheet>
      </form>
    </field>
  </record>
  
</odoo>